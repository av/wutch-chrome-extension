{
  "name": "Wutch",
  "version": "1.0.0",
  "description": "Wutch.net allows you to track any value on the Web.",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "manifest_version": 2,
  "icons": {
    "32": "images/logo_32.png",
    "48": "images/logo_48.png",
    "64": "images/logo_64.png",
    "128": "images/logo_128.png"
  },
  "offline_enabled": false,
  "browser_action": {}
}